{
  "name": "seo-ui",
  "version": "1.0.0",
  "description": "SEO UI code.",
  "author": "AOS Publishing Systems <AOS_Content_Engineering@group.apple.com>",
  "repository": {
    "type": "git",
    "url": "git+https://storegit.apple.com:4443/cs/seo-ui.git"
  },
  "scripts": {
    "install": "./build/update-pui.sh",
    "build": "steal-tools",
    "debug": "steal-tools --watch",
    "start": "http-server"
  },
  "aliasify": {
    "verbose": false,
    "aliases": {
      "components": "./src/components",
      "models": "./src/models",
      "utils": "./src/utils",
      "pages": "./src/pages",
      "test": "./test",
      "canjs": "can",
      "canjs/stache.js": "can/view/stache/stache",
      "canjs/map/define.js": "can/map/define/define",
      "canjs/fixture.js": "can/util/fixture/fixture",
      "canjs/construct/proxy.js": "can/construct/proxy/proxy",
      "canjs/construct/super.js": "can/construct/proxy/super"
    }
  },
  "browserify": {
    "transform": [
      "aliasify",
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "aliasify": "~1.7.2",
    "browser-sync": "~2.3.1",
    "browserify": "~9.0.3",
    "browserify-istanbul": "~0.2.1",
    "browserify-shim": "~3.8.3",
    "can-compilify": "~0.1.1",
    "codemirror": "~5.0.0",
    "del": "~1.1.1",
    "documentjs": "0.2.1",
    "esprima": "~2.2.0",
    "exorcist": "~0.4.0",
    "glob": "~5.0.3",
    "gulp": "~3.8.11",
    "gulp-autoprefixer": "~2.1.0",
    "gulp-concat": "~2.5.2",
    "gulp-connect": "~2.2.0",
    "gulp-debug": "~2.0.1",
    "gulp-if": "~1.2.5",
    "gulp-jshint": "~1.9.2",
    "gulp-less": "~3.0.1",
    "gulp-notify": "~2.2.0",
    "gulp-rename": "~1.2.0",
    "gulp-replace": "~0.5.3",
    "gulp-sourcemaps": "~1.5.0",
    "gulp-streamify": "~0.0.5",
    "gulp-uglify": "~1.1.0",
    "gulp-util": "~3.0.4",
    "gulp-xo": "^0.4.0",
    "jasmine-jquery": "~2.0.6",
    "jshint-stylish": "~1.0.1",
    "map-stream": "~0.0.5",
    "merge2": "^0.3.2",
    "minimist": "~1.1.1",
    "pretty-hrtime": "~1.0.0",
    "proxy-middleware": "~0.11.0",
    "require-dir": "~0.1.0",
    "run-sequence": "~1.1.0",
    "through": "~2.3.6",
    "through2": "~0.6.3",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~2.4.0",
    "xtend": "~4.0.0",
    "steal": "^0.16.22",
    "steal-jasmine": "0.0.2",
    "steal-tools": "^0.16.5",
    "testee": "^0.2.5"
  },
  "dependencies": {
    "bootstrap": "3.3.4",
    "can": "~2.3.24",
    "can-validate": "^0.8.3",
    "jquery": "~2.1.3",
    "lodash": "~3.5.0",
    "mocha-bamboo-reporter": "^1.1.1",
    "meld": "^1.3.2",
    "moment": "~2.9.0",
    "moment-duration-format": "^1.3.0",
    "moment-timezone": "~0.3.1",
    "pui": "git+https://storegit.apple.com/scm/cs/publishing-ui.git#develop"
  },
  "xo": {
    "envs": [
      "browser",
      "jasmine",
      "jquery",
      "phantomjs",
      "commonjs"
    ],
    "space": 4,
    "rules": {
      "one-var": [
        0,
        "never"
      ],
      "quote-props": [
        0,
        "consistent"
      ],
      "no-mixed-requires": 0,
      "space-before-function-paren": 0
    }
  },
  "system": {
  "npmAlgorithm": "flat",
  "npmIgnore": [
    "browser-sync",
    "del",
    "documentjs",
    "glob",
    "gulp",
    "gulp-autoprefixer",
    "gulp-less",
    "gulp-minify-css",
    "gulp-notify",
    "gulp-rename",
    "gulp-replace",
    "gulp-sourcemaps",
    "gulp-util",
    "gulp-jshint",
    "through2",
    "merge2",
    "pretty-hrtime",
    "require-dir",
    "run-sequence",
    "stream-from-promise",
    "testee"
  ],
  "bundlesPath": "./target",
  "main": "app",
  "directories": {
    "lib": "src"
  },
  "meta": {
    "jasmine-jquery/lib/jasmine-jquery": {
      "deps": [
        "jquery",
        "steal-jasmine"
      ]
    }
  }
}
}
