{
  "name": "seo-ui",
  "version": "1.0.0",
  "description": "SEO UI code.",
  "author": "AOS Publishing Systems <AOS_Content_Engineering@group.apple.com>",
  "repository": {
    "type": "git",
    "url": "git+https://storegit.apple.com:4443/cs/seo-ui.git"
  },
  "scripts": {
    "install": "./build/update-pui.sh",
    "build": "steal-tools",
    "debug": "steal-tools --watch",
    "start": "http-server"
  },
  "devDependencies": {
    "browser-sync": "2.16.0",
    "codemirror": "5.0.0",
    "del": "1.2.1",
    "documentjs": "0.4.2",
    "es5-shim": "4.5.9",
    "esprima": "2.2.0",
    "exorcist": "0.4.0",
    "glob": "5.0.15",
    "glob-all": "3.0.3",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "2.3.1",
    "gulp-concat": "2.5.2",
    "gulp-connect": "2.2.0",
    "gulp-debug": "2.0.1",
    "gulp-if": "1.2.5",
    "gulp-jshint": "1.10.0",
    "gulp-less": "3.1.0",
    "gulp-notify": "2.2.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-sourcemaps": "1.6.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "1.1.0",
    "gulp-util": "3.0.7",
    "gulp-xo": "0.4.0",
    "jasmine-jquery": "2.1.1",
    "jshint-stylish": "1.0.2",
    "map-stream": "0.0.5",
    "merge-stream": "1.0.0",
    "merge2": "0.3.7",
    "minimist": "1.2.0",
    "phantomjs": "1.9.20",
    "pretty-hrtime": "1.0.2",
    "proxy-middleware": "0.11.0",
    "require-dir": "0.1.0",
    "run-sequence": "1.2.2",
    "steal": "0.16.35",
    "steal-jasmine": "0.0.2",
    "steal-tools": "0.16.8",
    "testee": "0.2.4",
    "through": "2.3.6",
    "through2": "0.6.5",
    "vinyl-source-stream": "1.1.0",
    "watchify": "2.4.0",
    "xtend": "4.0.1"
  },
  "dependencies": {
    "bootstrap": "3.3.4",
    "can": "2.3.26",
    "can-validate": "0.8.3",
    "jquery": "2.1.4",
    "lodash": "3.5.0",
    "meld": "1.3.2",
    "mocha-bamboo-reporter": "1.1.1",
    "moment": "2.9.0",
    "moment-duration-format": "1.3.0",
    "moment-timezone": "0.5.0",
    "pui": "git+https://storegit.apple.com/scm/cs/publishing-ui.git#develop"
  },
  "xo": {
    "envs": [
      "browser",
      "jasmine",
      "jquery",
      "phantomjs",
      "commonjs"
    ],
    "space": 4,
    "rules": {
      "one-var": [
        0,
        "never"
      ],
      "quote-props": [
        0,
        "consistent"
      ],
      "no-mixed-requires": 0,
      "space-before-function-paren": 0
    }
  },
  "system": {
    "npmAlgorithm": "flat",
    "npmIgnore": [
      "browser-sync",
      "del",
      "documentjs",
      "glob",
      "gulp",
      "gulp-autoprefixer",
      "gulp-less",
      "gulp-minify-css",
      "gulp-notify",
      "gulp-rename",
      "gulp-replace",
      "gulp-sourcemaps",
      "gulp-util",
      "gulp-jshint",
      "through2",
      "merge2",
      "pretty-hrtime",
      "require-dir",
      "run-sequence",
      "stream-from-promise",
      "testee"
    ],
    "bundlesPath": "./target",
    "main": "app",
    "directories": {
      "lib": "src"
    },
    "meta": {
      "jasmine-jquery/lib/jasmine-jquery": {
        "deps": [
          "jquery",
          "steal-jasmine"
        ]
      }
    }
  }
}
